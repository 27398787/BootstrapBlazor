@namespace BootstrapBlazor.Components
@inherits IdComponentBase

<span @attributes="@AdditionalAttributes" class="@ClassString" tabindex="0" id="@Id" data-bs-selector="@Selector"
      data-bs-toggle="@ToggleString" data-bs-placement="@PlacementString" data-bs-customclass="@CustomClass"
      data-bs-trigger="@Trigger" data-bs-html="@HtmlString" data-bs-delay="@Delay" data-bs-sanitize="@SanitizeString">
    <CascadingValue Value="this" IsFixed="true">
        @ChildContent
    </CascadingValue>
</span>
